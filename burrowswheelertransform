wordinput = raw_input("the word: ")

counter = 0
cats = ""
wordlist = []

word0 = wordinput + "$"

for i in word0:
 counter = counter + 1

cats = word0

#for j in range(counter):
for i in range(counter):
 cats = cats[1:] + cats[0]
 wordlist.append(cats)

RADcat = wordlist
RADcat.sort()

CoolCat = ""

for i in RADcat:
 CoolCat = CoolCat + i[counter - 1]


print RADcat
print CoolCat
