file  = open ("2muscleout.aln","r")
x = file.readlines()
species = []
new = ""
meme = ""
counter = 0
specounter = -1
n = -1
lettercount = -1
dif = 0

for i in x:
 if counter != 0:
  meme = meme + i
 counter = counter + 1
 if counter == 12:
  species.append(meme)
  counter = 0
  meme = ""

for i in species:
 specounter = specounter + 1
tempspe = specounter

for i in species[0]:
 lettercount = lettercount + 1

for i in range(0,specounter+1):
  for j in species[i]:
   while tempspe != i:
    while n != lettercount:
     n = n + 1
     if j != species[tempspe][n]:
      dif = dif + 1
    print "Differences between",i,tempspe,":",dif
    dif = 0
    n = -1
    tempspe = tempspe - 1
   tempspe = specounter

